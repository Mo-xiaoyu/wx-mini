<view class='contentBottom'>
  <view class='BottomFirst'>
    <text id='0' data-id='0'>获奖名单</text>
  </view>
  <swiper class="tab-right" style='background:{{bgColor}};' vertical="true" autoplay="true" circular="true"
    interval="2000" display-multiple-items='4'>
    <view class="right-item">
      <block wx:for='{{aa}}' wx:key="idx">
        <swiper-item>
          <navigator class='content-item'>
            <text class='name'>{{item.nickName}}</text>
            <text class='wawa'>获得奖金{{item.reward}}元</text>
          </navigator>
        </swiper-item>
      </block>
    </view>
  </swiper>
</view>